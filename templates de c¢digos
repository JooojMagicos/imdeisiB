                                    boolean existe = false;
                                    for (ConstructorAtor atorExistente : objetoAtores) {
                                        if (atorExistente.getIdActor() == id) {
                                            existe = true;
                                            break;
                                        }
                                    }

                                    if (!existe) {
                                        ConstructorAtor ator = new ConstructorAtor(id, nome, genero,idFilme);
                                        objetoAtores.add(ator);
                                    }